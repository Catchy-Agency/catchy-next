{
  "name": "catchy-next",
  "version": "1.0.0",
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "clean": "rm -rf .next",
    "types": "tsc --noEmit",
    "types:watch": "tsc --noEmit --watch",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "fix": "npm run format && npm run lint",
    "schema-download": "export $(cat .env.local | xargs) && apollo schema:download --endpoint=\"https://graphql.datocms.com/\" --header=\"Authorization: Bearer $DATOCMS_API_TOKEN\"",
    "schema-codegen": "apollo codegen:generate --localSchemaFile=schema.json --target=typescript --useReadOnlyTypes --includes=gql/queries/**/*.ts --tagName=gql --addTypename --outputFlat gql/types && rm ./gql/types/globalTypes.ts",
    "schema": "npm run schema-download && npm run schema-codegen",
    "deploy:prod": "vercel --prod",
    "deploy:dev": "vercel"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link-context": "1.0.20",
    "apollo-link-http": "1.5.17",
    "bulma": "0.9.2",
    "classnames": "2.3.1",
    "graphql": "15.5.0",
    "graphql-tag": "2.11.0",
    "next": "10.1.3",
    "nprogress": "0.2.0",
    "react": "17.0.2",
    "react-datocms": "1.6.0",
    "react-dom": "17.0.2",
    "react-markdown": "6.0.1"
  },
  "devDependencies": {
    "@types/classnames": "2.3.0",
    "@types/node": "14.14.41",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "apollo": "2.32.8",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "next-secure-headers": "2.2.0",
    "next-sitemap": "1.6.25",
    "prettier": "2.2.1",
    "sass": "1.32.11",
    "typescript": "4.2.4",
    "vercel": "22.0.1"
  },
  "license": "MIT"
}
