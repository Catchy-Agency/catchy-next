{
  "name": "catchy-next",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "clean": "rm -rf .next",
    "types": "tsc --watch --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "schema-download": "export $(cat .env.local | xargs) && apollo schema:download --endpoint=\"https://graphql.datocms.com/\" --header=\"Authorization: Bearer $DATOCMS_API_TOKEN\"",
    "schema-codegen": "apollo codegen:generate --localSchemaFile=schema.json --target=typescript --useReadOnlyTypes --includes=gql/queries/**/*.ts --tagName=gql --addTypename --outputFlat gql/types && rm ./gql/types/globalTypes.ts",
    "schema": "npm run schema-download && npm run schema-codegen",
    "deploy-prod": "vercel --prod",
    "deploy-dev": "vercel"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link-context": "1.0.20",
    "apollo-link-http": "1.5.17",
    "bulma": "0.9.2",
    "classnames": "2.2.6",
    "graphql": "15.4.0",
    "graphql-tag": "2.11.0",
    "next": "10.0.5",
    "nprogress": "0.2.0",
    "react": "17.0.1",
    "react-datocms": "1.4.7",
    "react-dom": "17.0.1",
    "react-markdown": "5.0.3"
  },
  "devDependencies": {
    "@types/classnames": "2.2.11",
    "@types/node": "14.14.22",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "apollo": "2.32.1",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "sass": "1.32.6",
    "typescript": "4.1.3",
    "vercel": "21.2.0"
  },
  "license": "MIT"
}
