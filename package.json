{
  "name": "catchy-next",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "clean": "rm -rf .next",
    "types": "tsc --watch --noEmit",
    "schema-download": "export $(cat .env.local | xargs) && apollo schema:download --endpoint=\"https://graphql.datocms.com/\" --header=\"Authorization: Bearer $DATOCMS_API_TOKEN\"",
    "schema-codegen": "apollo codegen:generate --localSchemaFile=schema.json --target=typescript --useReadOnlyTypes --includes=gql/**/*.ts --tagName=gql --addTypename --globalTypesFile=./gql/types/global.ts --outputFlat gql/types",
    "schema": "npm run schema-download && npm run schema-codegen",
    "deploy": "vercel --prod"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link-context": "1.0.20",
    "apollo-link-http": "1.5.17",
    "classnames": "2.2.6",
    "graphql": "15.4.0",
    "graphql-tag": "2.11.0",
    "next": "10.0.5",
    "react": "17.0.1",
    "react-datocms": "1.4.7",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@types/classnames": "2.2.11",
    "@types/node": "14.14.22",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "apollo": "2.32.1",
    "autoprefixer": "10.2.3",
    "postcss": "8.2.4",
    "tailwindcss": "2.0.2",
    "typescript": "4.1.3",
    "vercel": "21.2.0"
  },
  "license": "MIT"
}
